{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "element-plus": "^1.0.2-beta.35",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "vue-router": "^4.0.0-beta.13",
    "vuex": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        2,
        2
      ],
      "no-console": 0,
      "no-new": 0,
      "promise/param-names": 0,
      "valid-typeof": 0,
      "comma-dangle": [
        0,
        "never"
      ],
      "semi": [
        0,
        "always"
      ],
      "eqeqeq": 0,
      "no-trailing-spaces": 0,
      "spaced-comment": 0,
      "padded-blocks": 0,
      "generator-star-spacing": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
